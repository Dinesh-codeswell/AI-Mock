{
  "component_type": "website_header_navigation",
  "component_purpose": "Primary navigation header for AI Mock Interview platform",
  "positioning": {
    "type": "scrollable_header",
    "behavior": "Scrolls with page content (not sticky/fixed)",
    "position": "relative",
    "z_index": "100",
    "width": "100%",
    "placement": "Top of webpage, first element in body"
  },
  "container": {
    "element": "header",
    "semantic_html": "Use <header> tag for accessibility",
    "max_width": "100%",
    "width": "100vw",
    "height": "72px",
    "background": {
      "color": "#FFFFFF",
      "type": "solid"
    },
    "border_bottom": {
      "width": "1px",
      "style": "solid",
      "color": "#E5E7EB"
    },
    "box_shadow": {
      "horizontal": "0",
      "vertical": "1px",
      "blur": "3px",
      "spread": "0",
      "color": "rgba(0, 0, 0, 0.05)",
      "description": "Subtle shadow for depth separation from content"
    },
    "padding": {
      "top": "0",
      "bottom": "0",
      "left": "32px",
      "right": "32px",
      "note": "Horizontal padding creates breathing room from viewport edges"
    },
    "display": "flex",
    "justify_content": "space-between",
    "align_items": "center"
  },
  "left_section_navigation_tabs": {
