  "center_section_search_bar": {
    "container": {
      "display": "flex",
      "align_items": "center",
      "flex": "1 1 auto",
      "max_width": "500px",
      "margin": {
        "left": "40px",
        "right": "40px"
      }
    },
    "search_input": {
      "element": "input",
      "type": "text",
      "placeholder": "Search interviews...",
      "styling": {
        "width": "100%",
        "height": "44px",
        "font_family": "sans-serif",
        "font_size": "15px",
        "font_weight": "400",
        "color": "#1F2937",
        "placeholder_color": "#9CA3AF",
        "background": "#FFFFFF",
        "border": {
          "width": "2px",
          "style": "solid",
          "color": "#E5E7EB",
          "note": "Light gray border by default"
        },
        "border_radius": "24px",
        "border_radius_description": "Fully rounded pill shape",
        "padding": {
          "top": "10px",
          "bottom": "10px",
          "left": "44px",
          "right": "16px",
          "note": "Extra left padding for search icon"
        },
        "outline": "none",
        "transition": "all 0.3s ease"
      },
      "search_icon": {
        "type": "SVG icon (magnifying glass)",
        "position": "absolute",
        "left": "16px",
        "top": "50%",
        "transform": "translateY(-50%)",
        "size": "20px",
        "color": "#9CA3AF",
        "stroke_width": "2px"
      },
      "focus_state": {
        "border_color": "#C084FC",
        "border_color_description": "Purple/pink gradient effect",
        "border_width": "2px",
        "box_shadow": "0 0 0 3px rgba(192, 132, 252, 0.1)",
        "outline": "none"
      },
      "hover_state": {
        "border_color": "#D1D5DB"
      }
    },
    "wrapper_position": "relative",
    "wrapper_display": "inline-block",
    "wrapper_width": "100%"
  },
  "right_section_actions": {
    "container": {
      "display": "flex",
      "align_items": "center",
      "gap": "12px",
      "flex": "0 0 auto"
    },
    "buttons": [
      {
        "upgrade_pro_button": {
          "label": "Upgrade Pro",
          "element": "button",
          "type": "secondary_cta",
          "styling": {
            "font_family": "sans-serif",
            "font_size": "15px",
            "font_weight": "600",
            "color": "#7C3AED",
            "color_description": "Purple text color",
            "background": "linear-gradient(135deg, #FAF5FF 0%, #F3E8FF 100%)",
            "background_description": "Very light purple gradient background",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "#C084FC",
              "description": "Light purple border"
            },
            "border_radius": "10px",
            "padding": {
              "top": "10px",
              "bottom": "10px",
              "left": "20px",
              "right": "20px"
            },
            "height": "44px",
            "min_width": "130px",
            "cursor": "pointer",
            "transition": "all 0.3s ease",
            "white_space": "nowrap"
          },
          "hover_state": {
            "background": "linear-gradient(135deg, #F3E8FF 0%, #E9D5FF 100%)",
            "border_color": "#A855F7",
            "transform": "translateY(-1px)",
            "box_shadow": "0 4px 12px rgba(124, 58, 237, 0.15)"
          },
          "active_state": {
            "transform": "translateY(0)",
            "box_shadow": "0 2px 6px rgba(124, 58, 237, 0.1)"
          }
        }
      },
      {
        "new_interview_button": {
          "label": "+ New Interview",
          "element": "button",
          "type": "primary_cta",
          "icon": {
            "symbol": "+",
            "position": "left of text",
            "spacing": "6px gap from text"
          },
          "styling": {
            "font_family": "sans-serif",
            "font_size": "15px",
            "font_weight": "600",
            "color": "#1F2937",
            "color_description": "Dark gray/black text",
            "background": "#FFFFFF",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "#1F2937",
              "description": "Dark border matching text"
            },
            "border_radius": "10px",
            "padding": {
              "top": "10px",
              "bottom": "10px",
              "left": "18px",
              "right": "18px"
            },
            "height": "44px",
            "min_width": "150px",
            "cursor": "pointer",
            "transition": "all 0.3s ease",
            "white_space": "nowrap",
            "display": "flex",
            "align_items": "center",
            "justify_content": "center",
            "gap": "6px"
          },
          "hover_state": {
            "background": "#1F2937",
            "color": "#FFFFFF",
            "border_color": "#1F2937",
            "transform": "translateY(-1px)",
            "box_shadow": "0 4px 12px rgba(0, 0, 0, 0.15)"
          },
          "active_state": {
            "transform": "translateY(0)",
            "box_shadow": "0 2px 6px rgba(0, 0, 0, 0.1)"
          }
        }
      },
      {
        "user_avatar_button": {
          "element": "button",
          "type": "user_profile_avatar",
          "styling": {
            "width": "44px",
            "height": "44px",
            "border_radius": "50%",
            "border_radius_description": "Perfect circle",
            "background": "linear-gradient(135deg, #14B8A6 0%, #3B82F6 50%, #8B5CF6 100%)",
            "background_description": "Teal to blue to purple gradient",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "#FFFFFF"
            },
            "display": "flex",
            "align_items": "center",
            "justify_content": "center",
            "cursor": "pointer",
            "transition": "all 0.3s ease",
            "overflow": "hidden"
          },
          "content": {
            "type": "initials",
            "text": "JD",
            "font_family": "sans-serif",
            "font_size": "15px",
            "font_weight": "700",
            "color": "#FFFFFF",
            "text_align": "center"
          },
          "dropdown_indicator": {
            "icon": "chevron-down arrow",
            "position": "Not visible in main avatar, separate element",
            "note": "Dropdown arrow shown separately if needed"
          },
          "hover_state": {
            "transform": "scale(1.05)",
            "box_shadow": "0 4px 12px rgba(0, 0, 0, 0.15)"
          },
          "note": "Can be replaced with actual profile image if available"
        }
      },
      {
        "dropdown_arrow": {
          "element": "button",
          "type": "dropdown_trigger",
          "position": "immediately after avatar",
          "visibility": "optional",
          "styling": {
            "width": "24px",
            "height": "24px",
            "background": "transparent",
            "border": "none",
            "cursor": "pointer",
            "padding": "0",
            "display": "none",
            "note": "If dropdown shown in image, include chevron-down icon"
          }
        }
      }
    ],
    "spacing": {
      "gap_between_elements": "12px",
      "note": "Consistent 12px gap between Upgrade Pro, New Interview, and Avatar"
    }
